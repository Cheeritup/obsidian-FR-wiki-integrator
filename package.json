{
	"name": "obsidian-fr-wiki-integrator",
	"version": "0.4.9",
	"description": "This is a plugin for importing articles from the Forgotten Realms wiki into Obsidian.md",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"test": "tsx runExample.ts",
		"debug": "tsx ",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"obsidian-typings": "^2.2.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@types/lodash": "^4.17.7",
		"lodash": "^4.17.21",
		"tsx": "^4.19.1"
	},
	"type": "module"
}
